{
  "name": "magnet",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm run generate-pages && tsc -b && vite build",
    "clear": "sh clear.sh",
    "generate-pages": "sh ./generate-pages.sh",
    "format:lint": "eslint . --fix",
    "format": "pnpm run \"/^format:.*/\"",
    "check:lint": "eslint .",
    "check": "pnpm run \"/^check:.*/\"",
    "preview": "vite preview",
    "prepare": "ts-patch install && typia patch",
    "deploy": "gh-pages -d dist -m \"deploy($(git rev-parse --abbrev-ref HEAD)): $(git rev-parse HEAD)\""
  },
  "dependencies": {
    "@unocss/reset": "^0.65.3",
    "chart.js": "^4.4.7",
    "date-fns": "^4.1.0",
    "dexie": "^4.0.10",
    "i18next": "^24.2.0",
    "react": "^19.0.0",
    "react-day-picker": "^9.4.4",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.2.0",
    "react-icons": "^5.4.0",
    "react-router": "^7.1.1",
    "typia": "^7.5.1",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@ryoppippi/unplugin-typia": "^1.1.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-perfectionist": "^4.4.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "gh-pages": "^6.2.0",
    "ts-patch": "^3.3.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "unocss": "^0.65.3",
    "vite": "^6.0.6"
  }
}
